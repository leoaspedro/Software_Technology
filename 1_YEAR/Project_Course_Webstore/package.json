{
  "name": "bikeshop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "cd functions && npm install",
    "check-format": "prettier --check src/**/*.{ts,tsx,css,html,js} && prettier --check e2e/**/*.{ts,tsx}",
    "format-code": "prettier --write src/**/*.{ts,tsx,css,html,js} & prettier --write e2e/**/*.{ts,tsx}",
    "build-prod": "ng build --prod",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "deploy-ci": "firebase use default && firebase deploy --only hosting --token $FIREBASE_TOKEN --non-interactive",
    "check-ci": "npm run check-format && npm run test-ci && npm run e2e-ci",
    "cleanupUsers": "node src/maintenance/cleanUsers.js",
    "cleanupProducts": "node src/maintenance/cleanupProducts.js",
    "createCategories": "node src/maintenance/createCategories.js",
    "scrape": "node src/maintenance/bikesterScraper.js",
    "seedProducts": "node src/maintenance/seedProducts.js"
  },
